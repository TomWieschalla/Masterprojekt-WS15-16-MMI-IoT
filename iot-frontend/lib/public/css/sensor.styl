#sensor
  .active
    color black
    &:hover
      color #2199e8
  .inactive
    color #cacaca

  .new-sensor-container
    position fixed
    z-index 10
    width 90%
    left 5%
    top 30px

  .background-area
    position absolute
    z-index 9
    top 0
    left 0
    width 100%
    height 100%
    background #fffd
